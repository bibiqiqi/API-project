<thead><tr>
<th width="200px" class="headerSort" tabindex="0" role="columnheader button" title="Sort ascending">Scientific name</th>
<th width="200px" class="headerSort" tabindex="0" role="columnheader button" title="Sort ascending">Name</th>
<th width="400px" class="headerSort" tabindex="0" role="columnheader button" title="Sort ascending">Description</th>
